.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_county_clerks_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_county_clerk_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_county_clerk_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      CountyClerk
    .inner
      = form_for @county_clerk, :url => admin_county_clerk_path(@county_clerk), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
