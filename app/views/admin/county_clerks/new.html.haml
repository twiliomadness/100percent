.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", county_clerks_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_county_clerk_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      CountyClerk
    .inner
      = form_for :county_clerk, :url => county_clerks_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
