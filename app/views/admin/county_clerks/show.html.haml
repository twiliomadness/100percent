.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_county_clerks_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_county_clerk_path
      %li.active= link_to "#{t("web-app-theme.show", :default => "Show")}", admin_county_clerk_path
  .content
    .inner
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.location_name", :default => t("activerecord.labels.location_name", :default => "Location name"))
          \:
        = county_clerk.location_name
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.address", :default => t("activerecord.labels.address", :default => "Address"))
          \:
        = county_clerk.address
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.city", :default => t("activerecord.labels.city", :default => "City"))
          \:
        = county_clerk.city
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.zip", :default => t("activerecord.labels.zip", :default => "Zip"))
          \:
        = county_clerk.zip
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.county", :default => t("activerecord.labels.county", :default => "County"))
          \:
        = county_clerk.county
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.phone_number", :default => t("activerecord.labels.phone_number", :default => "Phone number"))
          \:
        = county_clerk.phone_number
      %p
        %b
          = t("activerecord.attributes.admin_county_clerk.email_address", :default => t("activerecord.labels.email_address", :default => "Email address"))
          \:
        = county_clerk.email_address
      .wat-cf
        = link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Edit")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_admin_county_clerk_path(county_clerk), :class => "button"
        = link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), admin_county_clerk_path(county_clerk), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}"
- content_for :sidebar, render(:partial => 'sidebar')
